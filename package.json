    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@vue/cli-plugin-babel": "^3.0.0-beta.7",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.7",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.7",
    "@vue/cli-service": "^3.0.0-beta.7",
    "@vue/test-utils": "^1.0.0-beta.10",
